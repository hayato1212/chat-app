<div class = "header-bar">
  <div class = "user-name">
    <%= link_to current_user.name, edit_user_path(current_user) %>
  </div>
  <div class = "chat-link">
    <a href = "#">チャットを作成する</a>
  </div>
</div>

<div class = "main-bar">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
         <%= link_to room.name, room_messages_path(room) %>
      </div>
    </div>
  <% end %>
</div>
